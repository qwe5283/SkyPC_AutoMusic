<UserControl x:Class="SkyPC_Auto_Music.UserControl_Sheet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkyPC_Auto_Music"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="400">
    <Grid>
        <TextBlock x:Name="filePath" HorizontalAlignment="Center" VerticalAlignment="Center">Here are your sheets</TextBlock>
        <Button Width="100" Height="30" Margin="0,60,0,0" Click="BrowsePath">浏览</Button>
    </Grid>
</UserControl>
